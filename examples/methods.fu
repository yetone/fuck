method normal {
	$var := true

	if ($var) {
		out "\$var is true"
	else
		out "\$var is false"
	}

	return "ok"
} method

out "[ call normal ]"

method parameters $test $knark $3 {
	out "Method called, parameters is $test $knark $3"
} method

out "[ call parameters "ok", 1+1, "param3" ]"

$ok := "test " . "test2" . " test3"

$ko := 1 + 1 . "test" . [ call normal ] . [ normal ]

out $ok
out $ko
