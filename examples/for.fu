for $i; from 2; to 30; do $i + 1;
	out $i
end

array $arr [ "first", "second", "third" ]

for $key; in $arr
	out "$key"
end

for $key => $val; in $arr
	out "$key = $val"
end

for $key => $val; in [ "key1" => "item1" ]
	out "($key) = ($val)"
	for $key => $val; in [ "key2" => "item2", "key3" => "item3" ]
		out "($key) = ($val)"
	end
end