# push 3 different variables and print them

push a 5
push b 10
push var 15

out "printing variable values"
out "\$a=$a \$b=$b \$var=$var"

# print empty line
out

out "printing \$a+\$b+\$var"
push val "$a + $b + $var"
out $val

out "printing 10 + 5 + 1"
push val "10 + 5 + 1"
out $val

$another := 40
out $another

$var := 10
$var2 := 20

# Might parse "$var2" as $var + 2
# can result in 10, 102

out "$var, $var2"